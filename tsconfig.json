{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "ESNext",
    "target": "es2017",
    "lib": ["DOM", "es2017"],
    "jsx": "react",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "declaration": true,
    "sourceMap": true,
    "resolveJsonModule": true
  },
  "include": ["src/**/*"]
}
